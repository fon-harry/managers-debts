<div class="list-group">
    <% managersList.forEach(({id, name, link}) => {%>
        <a 
            href=<%= link %>
            <% if (id === managerData.managerId) { %>
                class="list-group-item list-group-item-action active"
            <% } else { %> 
                class="list-group-item list-group-item-action"
            <% } %> 
        > 
            <%= name %> 
        </a>
    <% }) %>
</div>